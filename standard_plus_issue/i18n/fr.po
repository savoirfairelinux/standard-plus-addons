#. module: standard_plus_issue
#: model:mail.template,body_html:standard_plus_issue.email_template_standard_plus_issue
msgid ""
"\n"
"        <h3>Request ${object.name}</h3>\n"
"        <p>\n"
"          A request of type <strong>${object.type}</strong> for project "
"${object.user_id.company_id.name} has been submitted by ${(object.user_id."
"name)} with a level '${object.priority}' priority.\n"
"        </p>\n"
"        <p>\n"
"          <strong>Concerned module:</strong>\n"
"          % if object.module_id.name:\n"
"            ${object.module_id.name}\n"
"          % else:\n"
"            left blank\n"
"          % endif\n"
"        </p>\n"
"        <p>\n"
"          <strong>Description of the observed behavior:</strong>\n"
"          <br>\n"
"          % if object.description_before:\n"
"            ${object.description_before}\n"
"          % else:\n"
"            left blank\n"
"          % endif\n"
"        </p>\n"
"        <p>\n"
"          <strong>Description of the desired behavior:</strong>\n"
"          <br>\n"
"          % if object.description_after:\n"
"            ${object.description_after}\n"
"          % else:\n"
"            left blank\n"
"          % endif\n"
"        </p>\n"
"        <p>\n"
"          % if object.screenshot_ids:\n"
"            <strong>Additional notes:</strong>\n"
"            % for screenshot in object.screenshot_ids:\n"
"              % if screenshot.comment:\n"
"                <br>\n"
"                <strong>${screenshot.filename}:</strong> ${screenshot."
"comment}\n"
"              % endif\n"
"            % endfor\n"
"          % endif\n"
"        </p>\n"
"        <p>\n"
"          ${object.issue_url}\n"
"        </p>\n"
"        "
msgstr ""
"\n"
"<h3>Demande ${object.name}</h3>\n"
"        <p>\n"
"          Une demande de type <strong>${object.type}</strong> concernant le "
"projet ${object.user_id.company_id.name} a été effectuée par l'utilisateur "
"${(object.user_id.name)} avec une priorité de niveau ${object.priority}.\n"
"        </p>\n"
"        <p>\n"
"          <strong>Module concerné:</strong>\n"
"          % if object.module_id.name:\n"
"          ${object.module_id.name}\n"
"          % else:\n"
"          non renseigné\n"
"          % endif\n"
"        </p>\n"
"        <p>\n"
"          <strong>Description du comportement observé:</strong>\n"
"          <br>\n"
"          % if object.description_before:\n"
"          ${object.description_before}\n"
"          % else:\n"
"          non renseigné\n"
"          % endif\n"
"        </p>\n"
"        <p>\n"
"          <strong>Description du comportement désiré:</strong>\n"
"          <br>\n"
"          % if object.description_after:\n"
"          ${object.description_after}\n"
"          % else:\n"
"          non renseigné\n"
"          % endif\n"
"        </p>\n"
"<p>\n"
"        % if object.screenshot_ids:\n"
"        <strong>Notes additionnelles:</strong>\n"
"        % for screenshot in object.screenshot_ids:\n"
"          % if screenshot.comment:\n"
"          <br>\n"
"          <strong>${screenshot.filename}:</strong> ${screenshot.comment}\n"
"          % endif\n"
"       % endfor\n"
"       % endif\n"
"        </p>\n"
"        <p>\n"
"           ${object.issue_url}\n"
"        </p>\n"
"        "

